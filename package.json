{
  "name": "cert-api",
  "version": "0.1.0",
  "description": "A comprehensive document authentication and management system API",
  "author": "Ephrem Bayru",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --exit-child --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "start:debug": "cross-env NODE_ENV=development ts-node-dev --inspect --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "yarn build && yarn typeorm migration:generate -d dist/src/data-source.js",
    "migration:run": "yarn build && npx typeorm migration:run -d dist/src/data-source.js",
    "migration:revert": "npx typeorm migration:revert",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts",
    "docs:generate": "typedoc --out docs src",
    "security:audit": "npm audit",
    "docker:build": "docker build -t cert-api .",
    "docker:run": "docker run -p 3000:3000 cert-api",
    "hardhat": "hardhat",
    "hardhat:compile": "hardhat compile",
    "hardhat:test": "hardhat test",
    "hardhat:node": "hardhat node",
    "hardhat:deploy": "hardhat run scripts/deploy.ts --network localhost",
    "hardhat:coverage": "hardhat coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.721.0",
    "@aws-sdk/client-ses": "^3.670.0",
    "@aws-sdk/s3-request-presigner": "^3.721.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/json2csv": "^5.0.7",
    "@types/multer": "^1.4.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.13.5",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.3",
    "cache-manager": "5",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "ethers": "^6.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "ioredis": "^5.3.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.3.3",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.7",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.9",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/ignition-core": "^0.15.9",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/chai": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "chai": "^4.2.0",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.22.18",
    "hardhat-gas-reporter": "^1.0.8",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "solidity-coverage": "^0.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.0",
    "typedoc": "^0.26.8",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.1.3",
    "webpack": "^5.95.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
